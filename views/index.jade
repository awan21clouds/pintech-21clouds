extends layout

block content
      .content-wrapper(ng-controller="pinController")
        // Main content
        section.content
          ul.mailbox-attachments.clearfix
            li(ng-repeat="pin in pins")
              span.mailbox-attachment-icon.has-img
                img(src='{{pin.url}}', alt='Attachment')
              .mailbox-attachment-info(style="background:#ffffff")
                span.mailbox-attachment-size
                  span(ng-bind="pin.title")
                  //span(ng-bind="book.title")
