doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/bootstrap/dist/css/bootstrap.min.css')
    link(rel='stylesheet', href='http://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css')
    link(rel='stylesheet', href='https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css')
    link(rel='stylesheet', href='/AdminLTE/dist/css/AdminLTE.min.css')
    link(rel='stylesheet', href='/AdminLTE/dist/css/skins/_all-skins.min.css')

    script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.1/jquery.js')
    script(src='/bootstrap/dist/js/bootstrap.min.js')
    script(src='/AdminLTE/plugins/slimScroll/jquery.slimscroll.min.js')
    script(src='/AdminLTE/dist/js/app.min.js')
    script(src='/AdminLTE/dist/js/demo.js')
    script(src='/angular/angular.min.js')
    script(src='/angular-route/angular-route.min.js')
  body.skin-blue.layout-top-nav
    .wrapper
      header.main-header
        nav.navbar.navbar-static-top
          .container
            .navbar-header
              a.navbar-brand(href='/')
                b Pin
                | Tech
              button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar-collapse')
                i.fa.fa-bars
            // Collect the nav links, forms, and other content for toggling
            #navbar-collapse.collapse.navbar-collapse.pull-left
              ul.nav.navbar-nav
                li.active
                  a(href='#')
                    | Recent Pins
                    span.sr-only (current)
                li
                  a(href='#') My Pins
            .navbar-custom-menu
              ul.nav.navbar-nav
                // Messages: style can be found in dropdown.less
                li.dropdown.messages-menu
                  // Menu toggle button
                  a.dropdown-toggle(href='#', data-toggle='dropdown')
                    i.fa.fa-envelope-o
                    span.label.label-success 4
                  ul.dropdown-menu
                    li.header You have 4 messages
                    li
                      // inner menu: contains the messages
                      .slimScrollDiv(style='position: relative; overflow: hidden; width: auto; height: 200px;')
                        ul.menu(style='overflow: hidden; width: 100%; height: 200px;')
                          li
                            // start message
                            a(href='#')
                              .pull-left
                                // User Image
                                img.img-circle(src='../../dist/img/user2-160x160.jpg', alt='User Image')
                              // Message title and timestamp
                              h4
                                | Support Team
                                small
                                  i.fa.fa-clock-o
                                  |  5 mins
                              // The message
                              p Why not buy a new awesome theme?
                          // end message
                        .slimScrollBar(style='width: 3px; position: absolute; top: 0px; opacity: 0.4; display: block; border-radius: 7px; z-index: 99; right: 1px; background: rgb(0, 0, 0);')
                        .slimScrollRail(style='width: 3px; height: 100%; position: absolute; top: 0px; display: none; border-radius: 7px; opacity: 0.2; z-index: 90; right: 1px; background: rgb(51, 51, 51);')
                      // /.menu
                    li.footer
                      a(href='#') See All Messages
                // /.messages-menu
                // Notifications Menu
                li.dropdown.notifications-menu
                  // Menu toggle button
                  a.dropdown-toggle(href='#', data-toggle='dropdown')
                    i.fa.fa-bell-o
                    span.label.label-warning 10
                  ul.dropdown-menu
                    li.header You have 10 notifications
                    li
                      // Inner Menu: contains the notifications
                      .slimScrollDiv(style='position: relative; overflow: hidden; width: auto; height: 200px;')
                        ul.menu(style='overflow: hidden; width: 100%; height: 200px;')
                          li
                            // start notification
                            a(href='#')
                              i.fa.fa-users.text-aqua
                              |  5 new members joined today
                          // end notification
                        .slimScrollBar(style='width: 3px; position: absolute; top: 0px; opacity: 0.4; display: block; border-radius: 7px; z-index: 99; right: 1px; background: rgb(0, 0, 0);')
                        .slimScrollRail(style='width: 3px; height: 100%; position: absolute; top: 0px; display: none; border-radius: 7px; opacity: 0.2; z-index: 90; right: 1px; background: rgb(51, 51, 51);')
                    li.footer
                      a(href='#') View all
                // Tasks Menu
                li.dropdown.tasks-menu
                  // Menu Toggle Button
                  a.dropdown-toggle(href='#', data-toggle='dropdown')
                    i.fa.fa-flag-o
                    span.label.label-danger 9
                  ul.dropdown-menu
                    li.header You have 9 tasks
                    li
                      // Inner menu: contains the tasks
                      .slimScrollDiv(style='position: relative; overflow: hidden; width: auto; height: 200px;')
                        ul.menu(style='overflow: hidden; width: 100%; height: 200px;')
                          li
                            // Task item
                            a(href='#')
                              // Task title and progress text
                              h3
                                | Design some buttons
                                small.pull-right 20%
                              // The progress bar
                              .progress.xs
                                // Change the css width attribute to simulate progress
                                .progress-bar.progress-bar-aqua(style='width: 20%', role='progressbar', aria-valuenow='20', aria-valuemin='0', aria-valuemax='100')
                                  span.sr-only 20% Complete
                          // end task item
                        .slimScrollBar(style='width: 3px; position: absolute; top: 0px; opacity: 0.4; display: block; border-radius: 7px; z-index: 99; right: 1px; background: rgb(0, 0, 0);')
                        .slimScrollRail(style='width: 3px; height: 100%; position: absolute; top: 0px; display: none; border-radius: 7px; opacity: 0.2; z-index: 90; right: 1px; background: rgb(51, 51, 51);')
                    li.footer
                      a(href='#') View all tasks
                // User Account Menu
                li.dropdown.user.user-menu
                  // Menu Toggle Button
                  a.dropdown-toggle(href='#', data-toggle='dropdown')
                    // The user image in the navbar
                    img.user-image(src='../AdminLTE/dist/img/user2-160x160.jpg', alt='User Image')
                    // hidden-xs hides the username on small devices so only the image appears.
                    span.hidden-xs Alexander Pierce
                  ul.dropdown-menu
                    // The user image in the menu
                    li.user-header
                      img.img-circle(src='../AdminLTE/dist/img/user2-160x160.jpg', alt='User Image')
                      p
                        | Alexander Pierce - Web Developer
                        small Member since Nov. 2012
                    // Menu Body
                    li.user-body
                      .col-xs-4.text-center
                        a(href='#') Followers
                      .col-xs-4.text-center
                        a(href='#') Sales
                      .col-xs-4.text-center
                        a(href='#') Friends
                    // Menu Footer
                    li.user-footer
                      .pull-left
                        a.btn.btn-default.btn-flat(href='#') Profile
                      .pull-right
                        a.btn.btn-default.btn-flat(href='#') Sign out
      block content
